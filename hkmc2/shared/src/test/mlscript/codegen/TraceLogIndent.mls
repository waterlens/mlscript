:js

fun plus(x, y) = x + y

:traceJS
fun g(z) =
  if
    z < -1 then g(z + 1)
    z == -1 then 42
    z == 0 then plus(0)
    z > 0 then g(z - 1)

fun f(x, y)(z) =
  x + y + g(z)

:traceJS
fun h(x) =
  f(1,2)(x)

