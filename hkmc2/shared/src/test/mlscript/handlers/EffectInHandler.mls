:js
:handler

class PrintEffect with
  fun p(s: String): ()
  fun aux(s: String): ()

:e
// :sjs
handle h = PrintEffect with
  fun p(x)(k) =
    print(x)
    k(())
  fun aux(x)(k) =
    h.p(x)
    k(())
    h.p(x)
h.aux(3)
