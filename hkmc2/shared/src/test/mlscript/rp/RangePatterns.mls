:js

pattern Lower = "a"..="z"

"a" is Lower
//│ = true

"z" is Lower
//│ = true

"0" is Lower
//│ = false

pattern Upper = "A"..="Z"

"A" is Upper
//│ = true

"Q" is Upper
//│ = true

"b" is Upper
//│ = false

pattern UnsignedShort = 0 ..< 65536

0 is UnsignedShort
//│ = true

-1 is UnsignedShort
//│ = false

65535 is UnsignedShort
//│ = true

65536 is UnsignedShort
//│ = false

2147483647 is UnsignedShort
//│ = false

"b" is UnsignedShort
//│ = false

[] is UnsignedShort
//│ = true

