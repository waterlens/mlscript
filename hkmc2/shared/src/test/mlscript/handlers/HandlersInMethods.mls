:js
:handler


abstract class Effect with
  fun perform(arg: Str): Str


class Test with
  fun p(x) =
    handle h = Effect with
      fun perform(arg, x)(k) =
        print(arg)
        k(x * 2)
    h.perform("A", x) + h.perform("B", x + 1)
let t = new Test
//│ t = Test {}

[t.p(2), t.p(3)]
//│ > A
//│ > B
//│ > A
//│ > B
//│ = [ 10, 14 ]


