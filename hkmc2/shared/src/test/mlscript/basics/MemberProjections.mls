:js


module M with
  class Foo(x) with
    fun m() = this.x + 1
    fun n(y) = this.x + y


let foo = M.Foo(123)
//│ foo = Foo { x: 123 }


foo.m()
//│ = 124

foo.n(1)
//│ = 124


foo M.Foo::m()
//│ = 124

foo M.Foo::n(2)
//│ = 125

M.Foo::n(foo, 2)
//│ = 125

M.Foo ::n(foo, 2)
//│ = 125

M.Foo ::n (foo, 2)
//│ = 125

:e
M.Foo:: n(foo, 2)
//│ ╔══[ERROR] Name not found: ::
//│ ║  l.37: 	M.Foo:: n(foo, 2)
//│ ╙──      	     ^^
//│ ╔══[ERROR] Name not found: n
//│ ║  l.37: 	M.Foo:: n(foo, 2)
//│ ╙──      	        ^


:sjs
let m = M.Foo::m
//│ JS (unsanitized):
//│ let m; m = (self, ...args) => { return self.m(...args); }; null
//│ m = [Function: m]

m(foo)
//│ = 124

foo m()
//│ = 124


M.{foo Foo::m()}
//│ = 124

M.{foo.Foo#m()}
//│ = 124

do
  open M
  print of foo M.Foo::m()
//│ > 124

do
  open M
  print of foo.Foo#m()
//│ > 124

// * This one still succeeds at runtime because the JS backend discards the class info
