

fun foo: Any -> Bool
fun (|>) pipe(x, f) = f(x)
fun abs(x) = if x < 0 then -x else x

:pt
:elt
fun example(args) =
  if foo(args)
    < 0 then "invalid"
    == 0 then "null"
    |> abs
      > 100 then "large"
      < 10 then "small"
      else "medium"
